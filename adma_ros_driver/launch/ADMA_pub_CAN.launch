<launch>
   <!-- parameters -->
   <arg name="path_to_adma_dbc_file" default="/SSD_512/Docs_SSD/ADMA_CAN-DBs/ADMA_CAN-DBC_ADMA_v3.2_v30.2.0.0.dbc" />
   <arg name="path_to_sensor_dbc_file" default="/SSD_512/Docs_SSD/ADMA_CAN-DBs/ADMA_CAN-DBC_ADMA_v3.2_v30.2.0.0.dbc" />
   <arg name="base_id_adma" default="100" />

   <!-- start node -->
	<node name="adma_can_connect" type="adma_can_connect" pkg="adma_ros_driver" output="screen" args="">
      <param name="path_to_adma_dbc_file" value="$(arg path_to_adma_dbc_file)" />
      <param name="path_to_sensor_dbc_file" value="$(arg path_to_sensor_dbc_file)" />
      <param name="base_id_adma" value="$(arg base_id_adma)" />
   </node>
</launch>
